select dpt_id,dpt_name from emp inner join dept on emp.dept_id = dept.dept_id group by dept_id where salary=max(dept.dept_id)

select dpt_id,dpt_name from emp inner join dept on emp.dept_id = dept.dept_id where salary in max(select max(salary) from from emp inner join dept on emp.dept_id = dept.dept_id group by dept_id)


